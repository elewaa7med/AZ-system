                        <div class="col-sm-9">
                            <p class="col-form-label col-sm-9 text-sm-left" id="lblDistrictCityCountry"></p>
                        </div>
                    </div>

                    <div class="form-group row">
                        <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Owner</label>
                        <div class="col-sm-9">
