@model SmartAdmin.WebUI.Models.UnitRentContract
@{
    ViewData["Title"] = "Contract Details";
    ViewData["PageName"] = "Compounds Rent Contracts";
    ViewData["Heading"] = $"<i class='fal fa-th-list text-primary'></i> Contract Details <a href=\"/UnitRentContractCompound/ContractPrint/{Model.IdRentContract}\" class=\"btn btn-primary waves-effect waves-themed\" style=\"float: right;\" target=\"_blank\">Print</a>";
    ViewData["Category1"] = "Compounds";
    ViewData["PageDescription"] = "";
    var canPay = (await AuthorizationService.AuthorizeAsync(User, null, new SmartAdmin.WebUI.Authorization.OperationAuthorizationRequirement { Permission = SmartAdmin.WebUI.Authorization.Permission.MakePayment })).Succeeded;
    var canPartialPay = (await AuthorizationService.AuthorizeAsync(User, null, new SmartAdmin.WebUI.Authorization.OperationAuthorizationRequirement { Permission = SmartAdmin.WebUI.Authorization.Permission.MakePartialPayment })).Succeeded;
}
<div class="was-validated">
    <input type="hidden" asp-for="IdRentContract" />
    <input type="hidden" asp-for="dtCreated" />
    <div class="row">
        <div class="col-xl-12">
            <div id="panel-6" class="panel ">
                <div class="panel-hdr bg-primary-700 bg-success-gradient">
                    <h2>
                        Unit Rent Contract
                    </h2>
                    <div class="panel-toolbar">
                        <button class="btn btn-panel waves-effect waves-themed" data-action="panel-fullscreen" data-toggle="tooltip" data-offset="0,10" data-original-title="Fullscreen"></button>
                    </div>
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="col-sm-6 ">
                            <div class="form-group row">
                                <label class="col-form-label col-sm-3 text-sm-right form-label">
                                    Building (A)
                                </label>
                                <div class="col-sm-9">
                                    <select class="select2 form-select w-100" asp-for="IdBuilding" disabled asp-items="ViewBag.IdBuildingsDDL" required>
                                        <optgroup label="Select compound">
                                        </optgroup>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                    Unit (B)
                                </label>
                                <div class="col-sm-9">
                                    <select class="select2 form-select w-100" asp-for="IdUnitCompound" disabled asp-items="ViewBag.IdUnitCompound" required>
                                        <optgroup label="Select Unit">
                                        </optgroup>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                    Tenant (C)
                                </label>
                                <div class="col-sm-9">
                                    <select class="select2 form-select w-100" asp-for="IdTenant" disabled asp-items="ViewBag.IdTenant" required>
