                </div>
            </div>
        </div>
    </div>
    @*notes*@
    <div class="card mb-g">
        <h4 class="card-header">Notes</h4>
        <div class="card-body">
            <div class="row ">
                <div class="col" id="js-contacts">
                    @foreach (var note in Model.UnitRentContractNotes.OrderByDescending(e => e.CreatedOn).Select((value, index) => new { value, index }))
                    {
                        <div class="col-xl-12">
                            <div id="@note.index" class="card border shadow-0 shadow-sm-hover mb-1" data-filter-tags="@note.value.User">
                                <div class="card-body border-faded border-top-0 border-left-0 border-right-0 rounded-top">
                                    <div class="d-flex flex-row align-items-center">
                                        <div class="info-card-text flex-1">
                                            <a href="javascript:void(0);" class="fs-xl text-truncate text-truncate-lg text-info" data-toggle="dropdown" aria-expanded="false">
                                                @note.value.User

                                            </a>
                                            <span class="text-truncate text-truncate-xl">@note.value.CreatedOn</span>
                                        </div>
                                    </div>
                                </div>
                                <div class="card-body p-0" style="">
                                    <div class="p-3">
                                        <span class="mt-1 d-block fs-sm fw-400 text-dark">
                                            @note.value.Note
                                        </span>
                                    </div>
                                </div>
                            </div>
                        </div>
                    }

                </div>
            </div>
        </div>
    </div>
    @*end Notes*@
    @*Payments*@
    <div class="card mb-g">
        <h4 class="card-header">
            Payments
              @if (Model.UnitRentContractPayments.Sum(x => x.Amount) != Model.UnitRentContractPayments.Sum(x => x.PaidAmount))
            {
                <a class="btn btn-success waves-effect waves-themed float-right" asp-action="PartiallyPay" title="Pay"  asp-route-ContractId=@Model.IdRentContract  style="margin:0px 20px;padding:9px 40px;font-weight:bold;">Pay</a>            
            }
        </h4>
        <div class="card-body">
            <div class="row ">
                <div class="col">
                    <div class="col">
                        <!-- datatable start -->
                        <table id="dt-basic-example" class="table table-bordered table-hover table-striped w-100 az-datatable">
                            <thead class="bg-primary-600">
                                <tr>
                                    <th>#</th>
                                    <th>Due Date </th>
                                    <th>Amount </th>
                                    <th>Remaining Amount </th>
                                    <th>Paid Amount </th>
                                    <th>Last payment Date </th>
                                    <th>User </th>
                                    <th>Comments </th>
                                    @*<th>Actions </th>*@

                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.UnitRentContractPayments.Select((value, i) => new { i, value }))
                                {
                                    <tr>
                                        <td>
                                            @(item.i + 1 )
                                        </td>
                                        <td>
                                            @item.value.DueDate.ToShortDateString()
                                        </td>
                                        <td class="formatNumber">
                                            @item.value.Amount
                                        </td>
                                        <td class="formatNumber">
                                            @item.value.RemainingAmount
                                        </td>
                                        <td class="paid formatNumber">
                                            @item.value.PaidAmount
                                        </td>
                                        <td>
                                            @item.value.PaymentDate?.ToShortDateString()
                                        </td>
                                        <td>
                                            @item.value.User?.fullName
                                        </td>
                                        <td title="@(item.value.Note??"")">
                                            @(item.value.Note?.Length>100? item.value.Note?.Substring(0,100)+"....": item.value?.Note)
                                        </td>
                                        @*<td>
                                            @if (!item.value.Paid)
                                            {
                                                <div class="btn-group" id="js-demo-nesting" role="group" aria-label="Button group with nested dropdown">
                                                    <div class="btn-group" role="group">
                                                        <button type="button" class="btn btn-secondary dropdown-toggle" data-toggle="dropdown">Operation</button>
                                                        <div class="dropdown-menu">
                                                            @if (canPay)
                                                            {
                                                                <a class="dropdown-item" asp-action="PartiallyPay" title="Pay" asp-route-id=@item.value.ID asp-route-fullPay="true">Pay</a>
                                                            }
                                                            @if (canPartialPay)
                                                            {
                                                                <a class="dropdown-item" asp-action="PartiallyPay" title="Partially Pay" asp-route-id=@item.value.ID>Partially Pay</a>
                                                            }
                                                        </div>
                                                    </div>
                                                </div>
                                            }
                                        </td>*@
                                    </tr>
                                }
                            </tbody>
                        </table>
                        <!-- datatable end -->
                    </div>
                </div>
            </div>
        </div>
    </div>
    @*end payments*@
