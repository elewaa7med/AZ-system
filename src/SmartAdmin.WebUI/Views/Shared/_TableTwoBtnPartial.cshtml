@model int
    <div class="btn-group">
        <a class="btn btn-sm btn-primary btn-success" asp-action="Details" title="Details" asp-route-id=@Model>
            <span class="glyphicon glyphicon-list"></span>
        </a>
        <a class="btn btn-sm btn-primary btn-danger" asp-action="Delete" title="Delete" asp-route-id=@Model>
            <span class="glyphicon glyphicon-trash"></span>
        </a>
    </div>