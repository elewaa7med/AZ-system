@model SmartAdmin.WebUI.Models.BuildingFloors

@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>

<h4>BuildingFloors</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Edit">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="IdBuildingFloor" />
            <div class="form-group">
                <label asp-for="PropertyFloorName" class="control-label"></label>
                <input asp-for="PropertyFloorName" class="form-control" />
                <span asp-validation-for="PropertyFloorName" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>


@section Scripts{
    <script>
        pageTitle.innerHTML = '<i class="fontello-icon-monitor"></i>' + 'Edit of a Floor ';
    </script>
}
