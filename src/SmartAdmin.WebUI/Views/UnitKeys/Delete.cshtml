@model SmartAdmin.WebUI.Models.UnitKeys

@{ ViewData["Title"] = $"Delete Unit Key";
    ViewData["PageName"] = "Keys " + ViewBag.id;
    ViewData["Heading"] = $"<i class='fal fa-th-list text-primary'></i> Delete Unit Key";
    ViewData["Category1"] = "Delete Unit Key";
    ViewData["PageDescription"] = "";
     }

<div class="was-validated">
    <div class="row">
        <div class="col-xl-12">
            <div id="panel-6" class="panel ">
                <div class="panel-hdr bg-primary-700 bg-success-gradient">
                    <h2>
                        Delete Unit Key
                    </h2>
                    <div class="panel-toolbar">
                        <button class="btn btn-panel waves-effect waves-themed" data-action="panel-fullscreen" data-toggle="tooltip" data-offset="0,10" data-original-title="Fullscreen"></button>
                    </div>
                </div>
                <div class="card-body">
                    <div class="row">
                        <div class="col-sm-6 ">
                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Building Country</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" required autocomplete="off" asp-for="@Model.mUnit.mBuilding.mDistrict.mCity.mBuildingCountry.CountryName" disabled>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label"> Building City</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" required autocomplete="off" asp-for="@Model.mUnit.mBuilding.mDistrict.mCity.CityName" disabled>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Building District</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" required autocomplete="off" asp-for="@Model.mUnit.mBuilding.mDistrict.DistrictName" disabled>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Building</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" required autocomplete="off" asp-for="@Model.mUnit.mBuilding.BuildingName" disabled>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Unite Number</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" required autocomplete="off" asp-for="@Model.mUnit.UnitNumber" disabled>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Mandoob Name</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" required autocomplete="off" asp-for="@Model.mMandoob.mandoobName" disabled>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Taken Date </label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" required autocomplete="off" asp-for="@Model.dtTaken" disabled>
                                </div>
                            </div>
                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Back Date </label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" required autocomplete="off" asp-for="@Model.dtBack" disabled>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <br>

                <div class="form-group  alert alert-primary alert-dismissible text-center">
                    <form asp-action="Delete">
                        <input type="hidden" asp-for="IdUnitKeys" />
                        <button class="btn btn-danger ml-1 waves-effect waves-themed" type="submit" asp-route-pageid=@ViewBag.id>Delete</button>
                        <a class="btn btn-secondary ml-1 waves-effect waves-themed" asp-action="Index" asp-route-id=@ViewBag.id>Cancel</a>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>
