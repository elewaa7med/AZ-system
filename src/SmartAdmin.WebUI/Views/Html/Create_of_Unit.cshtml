
@{
    ViewData["Title"] = "Create_of_Unit";

    ViewData["PageName"] = "Contract";
    ViewData["Heading"] = "<i class='fal fa-th-list text-primary'></i>  Create of a Building";
    ViewData["Category1"] = "html";
    ViewData["PageDescription"] = "";
}



<div class="row">
    <div class="col-xl-12">


        <div id="panel-6" class="panel ">
            <div class="panel-hdr bg-primary-700 bg-success-gradient">
                <h2>
                    Create New Building
                </h2>

                <div class="panel-toolbar">

                    <button class="btn btn-panel waves-effect waves-themed" data-action="panel-fullscreen" data-toggle="tooltip" data-offset="0,10" data-original-title="Fullscreen"></button>
                    <button class="btn btn-panel waves-effect waves-themed" data-action="panel-close" data-toggle="tooltip" data-offset="0,10" data-original-title="Close"></button>

                </div>


            </div>






            <div class="card-body">
                <form class="form-horizontal">

                    <div class="row">
                        <div class="col-sm-6 ">
                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label"> Building *</label>
                                <div class="col-sm-9">
                                    <select class="select2 form-control w-100" id="">
                                        <optgroup label="Sellect Lease Period In Monthes">
                                            <option value="CA">California</option>
                                            <option value="NV">Nevada</option>
                                        </optgroup>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                   Unit Number *

                                </label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">
                                </div>
                            </div>



                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                    Property Type *
                                </label>
                                <div class="col-sm-9">
                                    <select class="select2 form-control w-100" id="">
                                        <optgroup label="Sellect Lease Period In Monthes">
                                            <option value="CA">California</option>
                                            <option value="NV">Nevada</option>
                                        </optgroup>
                                    </select>
                                </div>
                            </div>


                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                  Floor *

                                </label>
                                <div class="col-sm-9">
                                    <select class="select2 form-control w-100" id="">
                                        <optgroup label="Sellect Lease Period In Monthes">
                                            <option value="CA">California</option>
                                            <option value="NV">Nevada</option>
                                        </optgroup>
                                    </select>
                                </div>

                            </div>

                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                    Unit Area
                                </label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">
                                </div>
                            </div>


                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                    Unit For Families 
                                </label>
                                <div class="col-sm-9">
                                    <div class="frame-wrap" style="padding-top:10px">
                                        <div class="custom-control custom-radio custom-control-inline">
                                            <input type="radio" class="custom-control-input" id="defaultInline1Radio" name="inlineDefaultRadiosExample">
                                            <label class="custom-control-label" for="defaultInline1Radio">Family</label>
                                        </div>
                                        <div class="custom-control custom-radio custom-control-inline">
                                            <input type="radio" class="custom-control-input" id="defaultInline2Radio" name="inlineDefaultRadiosExample" checked="">
                                            <label class="custom-control-label" for="defaultInline2Radio">Single</label>
                                        </div>

                                    </div>
                                </div>
                            </div>


                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                    Unit Condation *
                                </label>
                                <div class="col-sm-9">
                                    <select class="select2 form-control w-100" id="">
                                        <optgroup label="Sellect Lease Period In Monthes">
                                            <option value="CA">California</option>
                                            <option value="NV">Nevada</option>
                                        </optgroup>
                                    </select>
                                </div>
                            </div>

                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label"> Remarks</label>
                                <div class="col-sm-9">
                                    <textarea class="form-control" id="example-textarea" rows="5"></textarea>
                                </div>
                            </div>

                        </div>




                        <div class="col-sm-6">




                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label"> Number of Rooms *</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">
                                </div>
                            </div>

                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                    Number of Living *
                                </label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">

                                </div>
                            </div>


                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                    Number of Bathes *
                                </label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">

                                </div>
                            </div>


                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                    Number of Majlis*
                                </label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">

                                </div>
                            </div>



                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                    Rent Rate *
                                </label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">
                                </div>
                            </div>

                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                    Electricity Meter No


                                </label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">
                                </div>
                            </div>



                            <div class="form-group row">
                                <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                    Electricity Sadad No

                                </label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">
                                </div>
                            </div>





                        </div>
                    </div>



                    <br>

                    <div class="form-group  alert alert-primary alert-dismissible text-center">
                        <button class="btn btn-primary ml-auto waves-effect waves-themed" type="submit">Save Create Unit</button>
                        <button type="button" class="btn btn-secondary ml-1 waves-effect waves-themed">Cancel</button>
                    </div>

                </form>
            </div>


        </div>

    </div>
</div>

