
@{
    ViewData["Title"] = "Add_New_Tenant";
    ViewData["PageName"] = "Contract";
    ViewData["Heading"] = "<i class='fal fa-th-list text-primary'></i>  Add New Tenant";
    ViewData["Category1"] = "html";
    ViewData["PageDescription"] = "";
}


    <div class="row">
        <div class="col-xl-12">

            <div id="panel-6" class="panel ">
                <div class="panel-hdr bg-primary-700 bg-success-gradient">
                    <h2>
                        Add New Tenant

                    </h2>

                    <div class="panel-toolbar">

                        <button class="btn btn-panel waves-effect waves-themed" data-action="panel-fullscreen" data-toggle="tooltip" data-offset="0,10" data-original-title="Fullscreen"></button>
                        <button class="btn btn-panel waves-effect waves-themed" data-action="panel-close" data-toggle="tooltip" data-offset="0,10" data-original-title="Close"></button>

                    </div>


                </div>


                <div class="card-body">
                    <form class="form-horizontal">

                        <div class="row">
                            <div class="col-sm-6 ">
                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label"> Tenant Name *</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control " id="simpleinputInvalid" required="">
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                        Tenant Mobile *

                                    </label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">
                                    </div>
                                </div>



                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Emergency Phone</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">
                                    </div>
                                </div>


                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                        Nationality *
                                    </label>
                                    <div class="col-sm-9">
                                        <select class="select2 form-control w-100" id="">
                                            <optgroup label="Sellect Lease Period In Monthes">
                                                <option value="CA">California</option>
                                                <option value="NV">Nevada</option>
                                                <option value="OR" disabled="">Oregon(disabled)</option>
                                                <option value="WA">Washington</option>
                                                <option value="WA">Washington</option>
                                                <option value="WA">Washington</option>
                                            </optgroup>


                                        </select>
                                    </div>
                                </div>



                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Iqama No. *</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">
                                    </div>
                                </div>



                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">
                                        Company *

                                    </label>
                                    <div class="col-sm-9">
                                        <select class="select2 form-control w-100" id="">
                                            <optgroup label="Sellect Lease Period In Monthes">
                                                <option value="CA">California</option>
                                                <option value="NV">Nevada</option>

                                            </optgroup>


                                        </select>
                                    </div>
                                </div>


                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Tenant Remarks </label>
                                    <div class="col-sm-9">
                                        <textarea class="form-control" id="example-textarea" rows="5"></textarea>
                                    </div>
                                </div>

                            </div>




                            <div class="col-sm-6">




                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label"> Tenant Family Count *</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Tenant Email</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">

                                    </div>
                                </div>


                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">ID Letter</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">

                                    </div>
                                </div>


                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Work Notes</label>
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control " id="validationServer01" placeholder="First name" value=" " required="">

                                    </div>
                                </div>

                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Iqama Expiration Date *</label>
                                    <div class="col-sm-9">
                                        <div class="input-group">
                                            <input type="text" class="form-control az-datepicker-3 " readonly="" value="" id="">
                                            <div class="input-group-append">
                                                <span class="input-group-text fs-xl">
                                                    <i class="fal fa-calendar-alt"></i>
                                                </span>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Iqama Picture</label>
                                    <div class="col-sm-9">
                                        <div class="input-group">
                                            <input type="file" id="example-fileinput" class="form-control-file">

                                        </div>
                                    </div>
                                </div>

                                <div class="form-group row">
                                    <label for="inputEmail3" class="col-form-label col-sm-3 text-sm-right form-label">Tenant is Married? *</label>
                                    <div class="col-sm-9">
                                        <div class="frame-wrap">
                                            <div class="custom-control custom-radio custom-control-inline">
                                                <input type="radio" class="custom-control-input" id="defaultInline1Radio" name="inlineDefaultRadiosExample">
                                                <label class="custom-control-label" for="defaultInline1Radio"> Married</label>
                                            </div>
                                            <div class="custom-control custom-radio custom-control-inline">
                                                <input type="radio" class="custom-control-input" id="defaultInline2Radio" name="inlineDefaultRadiosExample" checked="">
                                                <label class="custom-control-label" for="defaultInline2Radio">Single</label>
                                            </div>
                                       
                                        </div>
                                    </div>
                                </div>





                            </div>
                        </div>



                        <br>

                        <div class="form-group  alert alert-primary alert-dismissible">
                            <div class="panel-content  border-left-0 border-right-0 border-bottom-0 d-flex flex-row align-items-center">
                                <div class="custom-control custom-checkbox">
                                    <input type="checkbox" class="custom-control-input" id="invalidCheck" required="">
                                    <label class="custom-control-label" for="invalidCheck"> Agree Create New Tenant <span class="text-danger">*</span></label>
                                    <div class="invalid-feedback">
                                        You must agree before submitting.
                                    </div>
                                </div>
                                <button class="btn btn-primary ml-auto waves-effect waves-themed" type="submit">Save New Tenant</button>
                                <button type="button" class="btn btn-secondary ml-1 waves-effect waves-themed">Cancel</button>
                            </div>
                        </div>

                    </form>
                </div>
            </div>

        </div>

    </div>

